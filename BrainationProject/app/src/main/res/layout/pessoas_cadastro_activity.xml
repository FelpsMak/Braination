<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/WallpaperNormal">
        <ImageView
            android:id="@+id/image_login_voltar_cadastro"
            android:layout_width="45dp"
            android:layout_height="45dp"
            android:layout_gravity="start"
            android:src="@drawable/ic_arrow_back_black_24dp"
            tools:ignore="MissingConstraints" />


        <LinearLayout
            android:id="@+id/linearLayout2"
            android:layout_width="360dp"
            android:layout_height="570dp"
            android:background="@drawable/borda"
            android:orientation="vertical"
            android:layout_marginTop="50dp"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:text="Como vai se cadastrar?"
                android:textColor="#FD050505"
                android:textSize="25sp" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="match_parent"
                android:layout_height="43dp"
                android:gravity="center"
                app:srcCompat="@drawable/linha" />

            <ImageView
                android:id="@+id/imageprof"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="professor"
                android:layout_marginTop="20dp"
                app:srcCompat="@drawable/component" />

            <ImageView
                android:id="@+id/imagecoord"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="coordenacao"
                android:layout_marginTop="20dp"
                app:srcCompat="@drawable/componente" />

            <ImageView
                android:id="@+id/imagealuno"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="alunos"
                android:layout_marginTop="20dp"
                app:srcCompat="@drawable/componenti" />

        </LinearLayout>
</LinearLayout>